Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

urxvt*scrollBar: false
urxvt*font: xft:Inconsolata:14
urxvt*dynamicColors: on

! material

urxvt.background: #000000
urxvt.foreground: #ffffff

! black
urxvt.color0:   #212121
urxvt.color8:   #9e9e9e

! red
urxvt.color1:   #e51c23
urxvt.color9:   #f69988

! green
urxvt.color2:   #259b24
urxvt.color10:  #72d572

! yellow
urxvt.color3:   #ffab00
urxvt.color11:  #ffd740

! blue
urxvt.color4:   #03a9f4
urxvt.color12:  #81d4fa

! magenta
urxvt.color5:   #aa00ff
urxvt.color13:  #e040fb

! cyan
urxvt.color6:   #00bcd4
urxvt.color14:  #80deea

! white
urxvt.color7:   #f5f5f5
urxvt.color15:  #ffffff
